#include <iostream>
using namespace std;

double hitungRataRata(int arr[], int ukuran) {
    int total = 0;
    for (int i = 0; i < ukuran; i++) {
        total += arr[i];
    }
    return (double)total / ukuran;
}
int main() {
    int arr[] = {5, 10, 15, 20, 25};
    int ukuran = sizeof(arr) / sizeof(arr[0]);
    
    cout << "Rata Rata nya: " << hitungRataRata(arr, ukuran) << endl;
    return 0;
}
